{
  "name": "@nl/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./button": "./src/button.tsx",
    "./card": "./src/card.tsx",
    "./code": "./src/code.tsx"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "format": "prettier --check \"**/*.{js,ts,tsx,md,mdx,json}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,tsx,md,mdx,json}\"",
    "type-check": "tsc --noEmit",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@nl/eslint-config": "*",
    "@nl/prettier-config": "*",
    "@nl/typescript-config": "*",
    "@turbo/gen": "^1.12.4"
  }
}
