    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-multi-carousel": "^2.8.6",
    "sharp": "^0.34.3",
    "three": "^0.178.0",
    "viem": "^2.33.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/three": "^0.178.1",
    "next-sitemap": "^4.2.3"
