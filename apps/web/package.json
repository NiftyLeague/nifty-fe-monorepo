{
  "name": "web",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf .next",
    "dev": "next dev --port 3000",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,mdx,json}\"",
