{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "clean": "rm -rf node_modules && rm -rf build && rm -rf .turbo && rm -rf .docusaurus",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start --no-open --port 3002",
    "docusaurus": "docusaurus",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,mdx,json}\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "type-check": "tsc --noEmit",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/plugin-google-tag-manager": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "algoliasearch": "^5.34.1",
    "lucide-react": "^0.525.0",
    "prism-react-renderer": "^2.4.1",
    "react-player": "^3.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^24.0.14",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
